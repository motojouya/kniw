{
  "name": "kniw",
  "version": "0.0.1",
  "description": "Node.js on Docker",
  "author": "Motojouya (Takuya Sugiyama) <motojouya@gmail.com>",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main.js",
    "build": "webpack",
    "prepare": "webpack --config webpack.config.js",
    "watch": "webpack --config webpack.dev.js --watch",
    "test": "mocha --require ts-node/register --require tsconfig-paths/register --watch-extensions ts 'test/**/*.ts'",
    "mocha": "mocha --require ts-node/register --extensions ts,tsx --watch --watch-files src 'test/**/*.ts'",
    "try": "ts-node -r tsconfig-paths/register",
    "manual": "ts-node -r tsconfig-paths/register manually_test/io/standard_dialogue.ts"
  },
  "private": true,
  "devDependencies": {
    "@types/node": "^17.0.35",
    "mocha": "^10.2.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@types/mocha": "^10.0.1",
    "@types/prompts": "^2.4.4",
    "commander": "^10.0.1",
    "prompts": "^2.4.2",
    "ts-node": "^10.9.1"
  }
}
