{
  "name": "kniw",
  "version": "0.0.1",
  "description": "Node.js on Docker",
  "author": "Motojouya (Takuya Sugiyama) <motojouya@gmail.com>",
  "main": "index.js",
  "scripts": {
    "kniw": "ts-node -r tsconfig-paths/register src/index.ts",
    "build": "webpack",
    "test": "mocha --require ts-node/register --require tsconfig-paths/register --watch-extensions ts 'test/**/*.ts'",
    "testsingle": "mocha --require ts-node/register --require tsconfig-paths/register --watch-extensions ts",
    "try": "ts-node -r tsconfig-paths/register",
    "manual": "ts-node -r tsconfig-paths/register manually_test/io/standard_dialogue.ts",
    "prettier": "npx prettier --write 'src/**/*.ts'",
    "eslint": "eslint --fix --ext .ts src"
  },
  "private": true,
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "mocha": "^10.2.0",
    "prettier": "^3.0.0",
    "ts-loader": "^9.3.0",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^11.0.0",
    "@types/mocha": "^10.0.1",
    "@types/prompts": "^2.4.4",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "commander": "^11.0.0",
    "date-fns": "^2.30.0",
    "json-schema-to-ts": "^2.9.1",
    "prompts": "^2.4.2",
    "shebang-loader": "^0.0.1",
    "ts-node": "^10.9.1"
  }
}
