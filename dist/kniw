        return;
    }
    let newCharactors = party.charactors.map(charactor => charactor.name);
    const fireOptions = party.charactors.map(charactor => ({
        value: charactor.name,
        label: charactor.name,
    }));
    const fireNames = await multiSelect('解雇するメンバーを複数選択してください。', fireOptions.length, fireOptions);
    if (!(fireNames instanceof standard_dialogue_1.NotApplicable) && fireNames.length !== 0) {
        newCharactors = newCharactors.filter(newName => !fireNames.includes(newName));
    }
    const charactorNames = await charactorStore.list();
    const hireOptions = charactorNames
        .filter(charactorName => !newCharactors.includes(charactorName))
        .map(charactorName => ({ value: charactorName, label: charactorName }));
    const hirableCount = 12 - newCharactors.length;
    const hireNames = await multiSelect(`雇うメンバーを複数選択してください。partyの残席は${hirableCount}名です。`, hirableCount, hireOptions);
    if (!(hireNames instanceof standard_dialogue_1.NotApplicable) && hireNames.length !== 0) {
        newCharactors = newCharactors.concat(hireNames);
    }
    const charactors = [];
    for (const newName of newCharactors) {
        // eslint-disable-next-line no-await-in-loop
        const charactor = await charactorStore.get(newName);
        if (!charactor) {
            // eslint-disable-next-line no-await-in-loop
            await notice(`${newName}というキャラクターはいません`);
            return;
        }
        if (charactor instanceof acquirement_1.NotWearableErorr ||
            charactor instanceof store_1.DataNotFoundError ||
            charactor instanceof store_1.JsonSchemaUnmatchError) {
            // eslint-disable-next-line no-await-in-loop
            await notice(charactor.message);
            return;
        }
        charactors.push(charactor);
    }
    const newParty = (0, party_2.createParty)(name, charactors);
    if (newParty instanceof party_2.CharactorDuplicationError) {
        await notice(newParty.message);
        return;
    }
    await partyStore.save(newParty);
    await notice(`${name}を組み直しました`);
};
exports.change = change;


