const createBattle = (title, home, visitor) => {
    const homeBatting = {
        name: home.name,
        charactors: home.charactors.map(charactor => (Object.assign(Object.assign({}, charactor), { isVisitor: false }))),
    };
    const visitorBatting = {
        name: visitor.name,
        charactors: visitor.charactors.map(charactor => (Object.assign(Object.assign({}, charactor), { isVisitor: true }))),
    };
    return {
        title,
        home: homeBatting,
        visitor: visitorBatting,
        turns: [],
        result: exports.GameOngoing,
    };
