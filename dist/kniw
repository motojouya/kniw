        turns.push((0, battle_1.wait)(battle, firstWaiting.restWt, new Date(), (0, random_1.createRandoms)()));
        const actor = (0, battle_1.nextActor)(battle);
        if ((0, status_1.underStatus)(sleep_1.sleep, actor)) {
            turns.push((0, battle_1.stay)(battle, actor, new Date()));
            await battleStore.save(battle);
        }
        else {
            const turn = await playerSelect(dialogue)(actor, battle);
            if (!turn) {
                break;
            }
            turns.push(turn);
            await battleStore.save(battle);
        }
        // eslint-disable-next-line no-param-reassign
        battle.result = (0, battle_1.isSettlement)(battle);
        if (battle.result !== battle_1.GameOngoing) {
            break;
        }
    }
    /* eslint-enable no-await-in-loop */
    await battleStore.save(battle);
    if (battle.result === battle_1.GameOngoing) {
        await dialogue.notice('勝負を中断しました');
    }
    if (battle.result === battle_1.GameHome || battle.result === battle_1.GameVisitor) {
        await dialogue.notice(`${battle.result}プレイヤーの勝利です`);
    }
    if (battle.result === battle_1.GameDraw) {
        await dialogue.notice('勝負は引き分けです');
    }
};
exports.continueBattle = continueBattle;
